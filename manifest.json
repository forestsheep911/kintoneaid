{
    "background": {
        "scripts": [
            "lib/jquery-3.4.1.min.js",
            "js/background.js"
        ]
    },
    "description": "happy kintone every day",
    "icons": {
        "16": "icon/t-16.png",
        "48": "icon/t-64.png",
        "128": "icon/t-128.png"
    },
    "manifest_version": 2,
    "name": "kintone aid",
    "browser_action": {
        "default_icon": {
            "16": "icon/t-16.png",
            "24": "icon/t-24.png",
            "32": "icon/t-32.png"
        }
    },
    "content_scripts": [{
            "matches": [
                "https://*.cybozu.com/k/*"
            ],
            "css": [
                "css/bigphoto.css"
            ],
            "js": [
                "lib/jquery-3.4.1.min.js",
                "js/photo_aid.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "version": "0.0.0.1"
}