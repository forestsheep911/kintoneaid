{
    "background": {
        "background_page": "background.html",
        "scripts": [
            "lib/jquery-3.4.1.min.js",
            "js/base/db.js",
            "js/background.js"
        ]
    },
    "default_locale": "en",
    "description": "__MSG_desName__",
    "icons": {
        "16": "icon/t-16.png",
        "48": "icon/t-64.png",
        "128": "icon/t-128.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "options_page": "option/option.html",
    "browser_action": {
        "default_icon": {
            "16": "icon/t-16.png",
            "24": "icon/t-24.png",
            "32": "icon/t-32.png"
        }
    },
    "content_scripts": [{
        "matches": [
            "https://*.cybozu.com/k/*",
            "https://*.cybozu.cn/k/*"
        ],
        "css": [
            "css/bigphoto.css"
        ],
        "js": [
            "lib/jquery-3.4.1.min.js",
            "js/base/db.js",
            "js/photo_aid.js",
            "js/at_aid.js",
            "js/mostuse_app_aid.js",
            "js/event_reg.js"
        ]
    }],
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "version": "0.0.0.93"
}